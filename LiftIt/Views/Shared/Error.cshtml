@model ErrorViewModel
@{
    ViewData["Title"] = "Błąd";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}


<link href="img/apple-touch-icon.png" rel="apple-touch-icon">
<link href="~/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
<link href="~/css/Index/animate.min.css" rel="stylesheet" />
<link href="~/css/Index/index.css" rel="stylesheet" />
<link href="~/css/Kalendarz/kalendarz.css" rel="stylesheet" />
<link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Poppins:300,400,500,700" rel="stylesheet">


<h1 class="text-danger">Błąd :()</h1>
<h2 class="text-danger">Twoje żądanie nie może zostać zrealizowane. Przepraszamy.</h2>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}

